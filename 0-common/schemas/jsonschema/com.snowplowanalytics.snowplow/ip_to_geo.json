{
	"$schema": "http://json-schema.org/schema#",
	"description": "Schema for an IP-to-geographical location enrichment",
	"self": {
		"vendor": "com.snowplowanalytics.snowplow",
		"name": "ip_to_geo",
		"format": "jsonschema",
		"version": "1-0-0"
	},

	"type": "object",
	"properties": {
		"vendor": {
			"type": "string"
		},
		"name": {
			"type": "string"
		},		
		"enabled": {
			"type": "boolean"
		},
		"parameters": {
			"type": "object",
			"properties": {
				"maxmindDatabase": {
					"type": "string"
				},
				"maxmindUri": {
					"type": "string"
				},
				"required": ["maxmindDatabase", "maxmindUri"],
				"additionalProperties": false
			}
		}
	},
	"required": ["name", "vendor", "enabled", "parameters"],
	"additionalProperties": false
}
